    <!--path name="voice-call-earpiece"-->
        <!-- Empty path V1A don't have this output-->
    <!--/path-->

    <!--path name="voice-call-earpiece-wb"-->
        <!-- Empty path V1A don't have this output-->
    <!--/path-->

    <path name="voice-call-speaker">
        <!-- FOR VOICE PHONE CALL -->
        <!-- For voice phone call OUTPUT Path -->
        <path name="OP-Speaker-Enable" />
        <path name="VC-HpSp" />
        <ctl name="Speaker Digital Volume" value="148" />
        <!-- #CP RX -->
        <ctl name="SPKOUTL Input 1 Volume" value="42" />
        <ctl name="SPKOUTR Input 1 Volume" value="42" />
        <path name="Verb-Voicecall" />
    </path>

    <path name="voice-call-speaker-wb">
        <!-- FOR VOICE PHONE CALL -->
        <path name="voice-call-speaker" />
    </path>

    <path name="voice-call-headphones">
        <!-- FOR VOICE PHONE CALL -->
        <!-- For voice phone call OUTPUT Path -->
        <path name="OP-Headphone-Enable" />
        <path name="VC-HpSp" />
        <ctl name="Noise Gate Switch" value="0" />
        <ctl name="HPOUT1 Digital Volume" value="148" />
        <!-- #CP RX -->
        <ctl name="HPOUT1L Input 3 Volume" value="32" />
        <ctl name="HPOUT1R Input 3 Volume" value="32" />
        <path name="Verb-Voicecall" />
    </path>

    <path name="voice-call-headphones-wb">
